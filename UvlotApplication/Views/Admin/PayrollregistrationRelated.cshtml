<script>
    $(document).ready(function(){
        var date_input = $('input[name="Sdate"],input[name="Edate"]'); //our date input has the name "date"
      var container=$('.bootstrap-iso form').length>0 ? $('.bootstrap-iso form').parent() : "body";
      var options={
        format: 'mm/dd/yyyy',
        container: container,
        todayHighlight: true,
        autoclose: true,
      };
      date_input.datepicker(options);
    })
</script>

<script type="text/javascript">
$(document).ready(function() {
$('table td:nth-child(8)').each(function() {
  let $this = $(this);

  // make sure it's a number
  if (!isNaN($this.text())) {

    // convert to number and check value
    if (+$this.text() > 20)
	{

	    var pieces = parseFloat($this.text()).toFixed(2).split('')
        let ii = pieces.length - 3
        while ((ii -= 3) > 0) {
            pieces.splice(ii, 0, ',')
        }
        $this.text(pieces.join(''));

    }
	else
	{
      $this.css('color', 'red');
    }
  }

});
});

</script>



<div class="charts">
    <div class="mid-content-top charts-grids">
        <div class="middle-content">
            @*<h4 class="title1">Pay Off</h4>*@
            <!-- Mega Form Starts -->

            <section class="section-padding contact-us-padding form-padding">

                <div class="container">
                    <div class="row justify-content-center">
                        <div class="col-md-12 col-lg-10">
                            @using (Html.BeginForm("PayrollregistrationRelated", "Admin", FormMethod.Post, new { id = "signup" }))
                            {
                                <form action="#" method="post">

                                    @if (TempData["ErrMsg"] != "")
                                    {
                                        <div id="Msgs" class="alert alert-danger">
                                            @TempData["ErrMsg"]
                                        </div>
                                    }
                                    @if (TempData["SucMsg"] != "")
                                    {
                                        <div id="Msgs" class="alert alert-success">
                                            @TempData["SucMsg"]
                                        </div>
                                    }
                                    <div class="tables">

                                        <div class="row list-input">
                                            <div class="col-md-6">
                                                <div class="single-get-touch">
                                                  
                                                    <input type="text" name="Sdate" id="Sdate" placeholder="DATE" autocomplete="off" readonly required="true" />
                                                </div>
                                            </div>

                                            <div class="col-md-6">
                                                <div class="single-get-touch">
                                                    <input type="text" name="Edate" id="Edate" placeholder="DATE" autocomplete="off" readonly required="true" />
                                                </div>
                                            </div>

                                            <div class="col-md-12">
                                                <div class="single-get-touch">
                                                    <a href="#"> <button type="submit" name="submit" class="btn btn-default btn-sm2 btnAction">Proceed</button></a>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </form>
                            }
                        </div>

                    </div>

                </div>
                <div class="tables">
                    <h2 class="title1"><h2 class="title1">Registration Related</h2></h2>
                    <div class="">
                      
                        <div class="table-responsive bs-example widget-shadow">
                            @*<h4>Users:</h4>*@
                            @{int i = 0;}
                            <table id="myTable" class="table table-bordered">

                                <thead>
                                    <tr>
                                        <th>No</th>
                                        <th>Registered Loan Customer</th>
                                        <th>New Account</th>
                                        <th>Partial Registration</th>
                                        <th>No Of Pending Applications</th>
                                       
                                    </tr>
                                </thead>
                                <tbody>

                                    @if (ViewBag.Data == null)
                                    {
                                        <h2>No Record Found</h2>
                                    }
                                    else
                                    {

                                        foreach (var LoanApp in ViewBag.Data)
                                        {
                                            <tr>
                                                <td scope="row">@(++i)</td>
                                                <td>@LoanApp.RgtLoanCustomer</td>
                                                <td>@LoanApp.NewAcct</td>
                                                <td>@LoanApp.PrtRegistration</td>
                                                <td>@LoanApp.NumOfPendActive</td>
                                              </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                            @if (ViewBag.Data != null)
                            {
                                <div class="col-md-2">
                                    <a href=@Url.Action("Exportoexcel", "Admin")>
                                        <button type="submit" class="btn btn-default" onchange="CallChangefunc()">Export To Excel</button>
                                    </a>
                                </div>
                            }
                        </div>
                                   
                        <div id="here" value="1"></div>
                    </div>
                </div>
            </section>

        </div>

    </div>

</div>




